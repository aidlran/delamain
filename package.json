{
  "name": "projex-web-client",
  "version": "1.0.0-PREVIEW",
  "private": true,
  "repository": "github:enclavetech/projex-web-client",
  "homepage": "https://github.com/enclavetech/projex-web-client#readme",
  "engines": {
    "node": "^18"
  },
  "type": "module",
  "scripts": {
    "build": "npm run build:libs && npm run build:app",
    "build:app": "vite build",
    "build:libs": "npm run build:lib:key-manager",
    "build:lib:key-manager": "cd packages/key-manager && npm run build",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "dev": "concurrently npm:dev:app npm:dev:libs",
    "dev:app": "vite dev",
    "dev:libs": "npm run dev:lib:key-manager",
    "dev:lib:key-manager": "cd packages/key-manager && npm run dev",
    "link": "npm i ./packages/key-manager",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "prepare": "npm run prisma:generate",
    "prettier": "prettier --plugin-search-dir . --write .",
    "preview": "vite preview",
    "prisma:generate": "prisma generate",
    "prisma:push": "prisma db push",
    "start": "npm run dev"
  },
  "dependencies": {
    "@prisma/client": "^4.12.0",
    "argon2": "^0.30.3",
    "class-validator": "^0.14.0",
    "class-validator-svelte": "github:aidlran/class-validator-svelte#preview",
    "jose": "^4.13.1",
    "key-manager": "file:packages/key-manager",
    "openpgp": "^5.7.0"
  },
  "devDependencies": {
    "@sveltejs/adapter-vercel": "^2.4.1",
    "@sveltejs/kit": "^1.15.2",
    "@types/estree": "^1.0.0",
    "@types/jsonwebtoken": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "concurrently": "^8.0.1",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-deprecation": "^1.4.1",
    "eslint-plugin-svelte3": "^4.0.0",
    "prettier": "^2.8.4",
    "prettier-plugin-svelte": "^2.9.0",
    "prisma": "^4.12.0",
    "svelte": "^3.58.0",
    "svelte-check": "^3.2.0",
    "tslib": "^2.5.0",
    "typescript": "^5.0.4",
    "vite": "^4.2.1"
  }
}
