{
  "name": "projex-web-client",
  "version": "1.0.0-PREVIEW",
  "private": true,
  "repository": "github:enclavetech/projex-web-client",
  "homepage": "https://github.com/enclavetech/projex-web-client#readme",
  "engines": {
    "node": "^18"
  },
  "type": "module",
  "scripts": {
    "build": "npm run build:libs && npm run build:app",
    "build:app": "vite build",
    "build:libs": "concurrently npm:build:lib:*",
    "build:lib:key-manager": "cd packages/key-manager && npm run build:core && npm run build:adapter:jose",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "dev": "concurrently -k npm:dev:app npm:dev:libs",
    "dev:app": "vite dev",
    "dev:libs": "npm run dev:lib:key-manager",
    "dev:lib:key-manager": "cd packages/key-manager && npm run dev",
    "lint": "concurrently npm:lint:app npm:lint:libs",
    "lint:app": "prettier --plugin-search-dir . --check . && eslint .",
    "lint:libs": "cd packages/key-manager && npm run lint",
    "prepare": "npm run prisma:generate",
    "prettier": "prettier --plugin-search-dir . --write .",
    "preview": "vite preview",
    "prisma:generate": "prisma generate",
    "prisma:push": "prisma db push",
    "start": "npm run dev"
  },
  "dependencies": {
    "@enclavetech/kms-core": "file:packages/key-manager/packages/core",
    "@enclavetech/kms-jose": "file:packages/key-manager/packages/adapters/jose",
    "@prisma/client": "^4.15.0",
    "@vercel/analytics": "^1.0.1",
    "argon2": "^0.30.3",
    "class-validator": "^0.14.0",
    "class-validator-svelte": "github:aidlran/class-validator-svelte#preview",
    "jose": "4.14.0"
  },
  "devDependencies": {
    "@sveltejs/adapter-vercel": "^3.0.1",
    "@sveltejs/kit": "^1.20.4",
    "@types/estree": "^1.0.1",
    "@typescript-eslint/eslint-plugin": ">=5.60.0",
    "@typescript-eslint/parser": ">=5.60.0",
    "concurrently": "^8.0.0",
    "eslint": ">=8.43.0",
    "eslint-config-prettier": ">=8.8.0",
    "eslint-plugin-deprecation": ">=1.4.1",
    "eslint-plugin-svelte3": ">=4.0.0",
    "prettier": ">=2.8.8",
    "prettier-plugin-prisma": ">=4.13.0",
    "prettier-plugin-svelte": ">=2.10.1",
    "prisma": "^4.15.0",
    "svelte": "^3.59.1",
    "svelte-check": "^3.4.3",
    "tslib": "^2.5.3",
    "typescript": "^5.1.3",
    "vite": "^4.3.9"
  }
}
