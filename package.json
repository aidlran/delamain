{
  "name": "@enclavetech/projex",
  "version": "1.0.0-PREVIEW",
  "private": true,
  "repository": "github:enclavetech/projex",
  "homepage": "https://github.com/enclavetech/projex#readme",
  "engines": {
    "node": "^18"
  },
  "type": "module",
  "workspaces": [
    "lib/*"
  ],
  "scripts": {
    "build": "npm run build:libs && npm run build:app",
    "build:app": "vite build",
    "build:libs": "concurrently npm:build:lib:*",
    "build:lib:class-validator-svelte": "cd lib/class-validator-svelte && npm run build",
    "build:lib:lib-web": "cd lib/web && npm run build",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "dev": "concurrently -k npm:check:watch npm:dev:app npm:dev:libs",
    "dev:app": "vite dev",
    "dev:libs": "concurrently npm:dev:lib:*",
    "dev:lib:class-validator-svelte": "cd lib/class-validator-svelte && npm run dev",
    "dev:lib:lib-web": "cd lib/web && npm run dev",
    "lint": "concurrently npm:lint:app npm:lint:libs",
    "lint:app": "prettier --plugin-search-dir . --check . && eslint .",
    "lint:libs": "concurrently npm:lint:lib:*",
    "lint:lib:class-validator-svelte": "cd lib/class-validator-svelte && npm run lint",
    "lint:lib:lib-web": "cd lib/web && npm run lint",
    "prepare": "concurrently npm:prepare:*",
    "prepare:app": "npm run prisma:generate",
    "prettier": "concurrently npm:prettier:app npm:prettier:libs",
    "prettier:app": "prettier --plugin-search-dir . --write .",
    "prettier:libs": "concurrently npm:prettier:lib:*",
    "prettier:lib:class-validator-svelte": "cd lib/class-validator-svelte && npm run prettier",
    "prettier:lib:lib-web": "cd lib/web && npm run prettier",
    "preview": "vite preview",
    "prisma:generate": "prisma generate",
    "prisma:push": "prisma db push",
    "start": "npm run dev",
    "submodules:init": "git submodule update --recursive --init",
    "submodules:update": "git submodule update --recursive"
  },
  "dependencies": {
    "@enclavetech/lib-web": "file:lib/web",
    "@prisma/client": "^4.15.0",
    "@vercel/analytics": "^1.0.1",
    "argon2": "^0.30.3",
    "class-validator": "^0.14.0",
    "class-validator-svelte": "file:lib/class-validator-svelte",
    "jose": "^4.14.0"
  },
  "devDependencies": {
    "@openpgp/web-stream-tools": "^0.0.13",
    "@sveltejs/adapter-vercel": "^3.0.1",
    "@sveltejs/kit": "^1.20.4",
    "@types/estree": "^1.0.1",
    "@typescript-eslint/eslint-plugin": ">=5.60.0",
    "@typescript-eslint/parser": ">=5.60.0",
    "concurrently": "*",
    "eslint": ">=8.43.0",
    "eslint-config-prettier": ">=8.8.0",
    "eslint-plugin-deprecation": ">=1.4.1",
    "eslint-plugin-svelte3": ">=4.0.0",
    "prettier": ">=2.8.8",
    "prettier-plugin-prisma": ">=4.13.0",
    "prettier-plugin-svelte": ">=2.10.1",
    "prisma": "^4.15.0",
    "svelte": "^3.59.1",
    "svelte-check": "^3.4.3",
    "tslib": "^2.5.3",
    "typescript": "~5.0.0",
    "vite": "^4.3.9"
  }
}
